<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Resala.Apps.VolCRM.Styles"
                    xmlns:views="clr-namespace:Resala.Apps.VolCRM.Views"
                    xmlns:viewModels="clr-namespace:Resala.Apps.VolCRM.ViewModels">


    <DataTemplate DataType="{x:Type viewModels:ProfileViewModel}">
        <views:ProfileView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:EventsViewModel}">
        <views:EventsView/>
    </DataTemplate>


    <DataTemplate DataType="{x:Type viewModels:CallsViewModel}">
        <views:CallsView/>
    </DataTemplate>



</ResourceDictionary>